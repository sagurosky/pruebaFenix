package com.cimientos.intranet.enumerativos.entrevista;

public class EventoGenericoDTO {
	
	private String id;
	private String lugarEvento;
	private String ea;
	private String tipoEvento;
	private String fechaCarga;
	private String fechaEvento;
	private String observaciones;
	private String otroContenido;
	private String evaluacion;
	private String contenido;
	private String convocados;
	private String asistio;
	private String responsable1;
	private String vinculo1;
	private String responsable2;
	private String vinculo2;
	private String zona;
