	/**
	 * Retorna el motivo que corresponda segun la renovacion
	 * @return
	 */
	public String getMotivoRenovacionUnificado() {
		if(this.situacionRenovacion != null && this.situacionRenovacion == EvaluacionRenovacionMergeada.PENDIENTE.getValor()){
			if(this.motivoPendiente != null && this.motivoPendiente != "")
				return this.motivoPendiente;
			else
				if(this.otroMotivo != null && this.otroMotivo != "")
					return "Otro," + this.otroMotivo;
				else
					return "";
		}
		else if(this.situacionRenovacion != null && this.situacionRenovacion == EvaluacionRenovacionMergeada.NO_RENUEVA.getValor()){
				if(this.motivoNoRenovacion != null && this.motivoNoRenovacion != "")
					return this.motivoNoRenovacion;
				else
					if(this.otroMotivo != null && this.otroMotivo != "")
						return "Otro," + this.otroMotivo;
					else
						return "";
			}
		else
			return "No aplica";		
	}
	
	/**
	 * Retorna el motivo de cesacion o suspencion segun corresponda
	 * @return
	 */
	public String getMotivoSuspensionCesacion() {
		if(this.evaluacionCobroBeca == EvaluacionCobroBeca.SUSPENDIDO.getValor())
			return this.motivoSuspension;
		else
			return this.motivoCesacion;
	}	
