	/**
	 * Retorno el motivo de incorporacion o no incorporacion. Dependiendo de la incorporacion es cual usa
	 * @return
	 */
	public String getMotivoIncorporacion() {
		if(this.incorporacion.equals(EvaluacionIncorporacion.INCORPORA.getValor()))
			return "";
		else
			if(this.incorporacion.equals(EvaluacionIncorporacion.INCORPORAPENDIENTE.getValor()))
				if(this.motivoIncorporacionPendiente != null && this.motivoIncorporacionPendiente != "" 
					&& this.motivoIncorporacionPendiente != MotivoIncorporacionPendiente.OTROMOTIVO.getValor())
					return this.motivoIncorporacionPendiente;
				else
					return "Otro," + this.otroMotivoPendiente;
			else
				if(this.motivoNoIncorporacion != null && this.motivoNoIncorporacion != ""
					&& this.motivoNoIncorporacion != "Otro")
					return this.motivoNoIncorporacion;
				else
					return "Otro," + this.otroMotivoNoIncorporacion;
				
	}
	
